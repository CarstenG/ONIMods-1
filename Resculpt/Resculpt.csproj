<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <AssemblyTitle>Resculpt</AssemblyTitle>
    <FileVersion>2.7.0.0</FileVersion>
    <RootNamespace>PeterHan.Resculpt</RootNamespace>
    <Description>Allows the design of sculpting blocks and paintings to be changed without reconstructing them.</Description>
    <AssemblyVersion>2.2.0.0</AssemblyVersion>
    <Platforms>Vanilla;Spaced Out</Platforms>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Platform)' == 'Vanilla' ">
    <ArchivedVersionPath>\archived_versions\vanilla</ArchivedVersionPath>
    <SupportedContent>VANILLA_ID</SupportedContent>
    <LastWorkingBuild>469300</LastWorkingBuild>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Platform)' == 'Spaced Out' ">
    <SupportedContent>EXPANSION1_ID</SupportedContent>
    <LastWorkingBuild>469301</LastWorkingBuild>
  </PropertyGroup>
  <ItemGroup>
    <None Remove="repaint.png" />
    <None Remove="resculpt.png" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="repaint.png" />
    <EmbeddedResource Include="resculpt.png" />
  </ItemGroup>
</Project>
